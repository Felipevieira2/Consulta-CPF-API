        "tor:linux": "bash ensure-tor.sh && node scraper-linux-tor.js",
        "tor:linux:visual": "bash ensure-tor.sh && VISUAL_MODE=true node scraper-linux-tor.js",
        "test:tor:linux": "bash ensure-tor.sh && node scraper-linux-tor.js 45083784807 29/03/1995",
        "test:tor:linux:visual": "bash ensure-tor.sh && VISUAL_MODE=true node scraper-linux-tor.js 45083784807 29/03/1995",
