        "linux:visual": "VISUAL_MODE=true node scraper-linux-chromium.js",
        "test:linux": "node scraper-linux-chromium.js 45083784807 29/03/1995",
        "test:linux:visual": "VISUAL_MODE=true node scraper-linux-chromium.js 45083784807 29/03/1995",
        "test:thorium": "node scraper-linux-chromium.js 45083784807 29/03/1995",
        "test:thorium:visual": "VISUAL_MODE=true node scraper-linux-chromium.js 45083784807 29/03/1995",
