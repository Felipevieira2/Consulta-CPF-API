{
    "name": "receita-federal-scraper",
    "version": "1.0.0",
    "description": "ServiÃ§o para consulta de CPF na Receita Federal",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "start:otimizado": "node server-otimizado.js",
        "start:stealth": "node server-stealth.js",
        "start:ip-rotation": "node server-ip-rotation.js",
        "dev": "nodemon server.js --legacy-watch",
        "dev:otimizado": "nodemon server-otimizado.js --legacy-watch",
        "dev:stealth": "nodemon server-stealth.js --legacy-watch",
        "dev:ip-rotation": "nodemon server-ip-rotation.js --legacy-watch",
        "scraper": "node scraper.js",
        "scraper:otimizado": "node scraper-otimizado.js",
        "scraper:visual": "node run-visual.js",
        "scraper:headless": "node run-headless.js",
        "test:cpf": "node scraper.js 45083784807 29/03/1995",
        "test:cpf:visual": "VISUAL_MODE=true node scraper.js 45083784807 29/03/1995",
        "test:cpf:otimizado": "node scraper-otimizado.js 45083784807 29/03/1995",
        "clean": "node limpar-navegador.js",
        "clean:fast": "node limpar-rapido.js",
        "clean:webkit": "npx playwright install webkit",
        "test:menu": "node teste-completo.js",
        "debug:captcha": "node debug-captcha.js",
        "stealth": "node scraper-stealth.js",
        "stealth:visual": "VISUAL_MODE=true node scraper-stealth.js",
        "test:stealth": "node scraper-stealth.js 45083784807 29/03/1995",
        "ip-rotation": "node scraper-ip-rotation.js",
        "ip-rotation:visual": "VISUAL_MODE=true node scraper-ip-rotation.js",
        "test:ip-rotation": "node scraper-ip-rotation.js 45083784807 29/03/1995",
        "test:proxies": "node -e \"const ProxyManager = require('./proxy-manager'); new ProxyManager().testAllProxies();\"",
        "exemplos": "node exemplos-integracao.js",
        "consulta-rapida": "node -e \"require('./scraper-stealth').consultarCPF('45083784807', '29/03/1995').then(console.log)\"",
        "consulta-ip": "node -e \"require('./scraper-ip-rotation').consultarCPF('45083784807', '29/03/1995').then(console.log)\"",
        "setup-proxies": "node setup-proxies.js",
        "test:anti-detect:visual": "VISUAL_MODE=true node test-stealth.js",
        "consulta:stealth": "node test-stealth.js",
        "consulta:stealth:visual": "VISUAL_MODE=true node test-stealth.js",
        "firefox": "node scraper-firefox.js",
        "firefox:visual": "VISUAL_MODE=true node scraper-firefox.js",
        "test:firefox": "node scraper-firefox.js 45083784807 29/03/1995",
        "test:firefox:visual": "VISUAL_MODE=true node scraper-firefox.js 45083784807 29/03/1995"
    },
