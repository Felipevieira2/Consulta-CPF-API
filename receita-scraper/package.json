        "puppeteer": "^21.0.0",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-anonymize-ua": "^2.4.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2",
        "rebrowser-playwright": "^1.52.0",
        "selenium-webdriver": "^4.35.0",
        "socks-proxy-agent": "^8.0.5",
        "tor-request": "^3.1.0"
